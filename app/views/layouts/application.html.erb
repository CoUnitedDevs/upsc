<!DOCTYPE html>
<html>
<head>
  <title>UPSC</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body class="nav-md login">
  <div class="container body">
    <div class="main_container ">
      <% if user_signed_in? %>
      <%= render partial: 'layouts/left_menu' %>
      <%= render partial: 'layouts/top_menu' %>
      <% end %>
      <!-- <%# @user_error_messages =  ""%> -->
      <div class="right_col" role="main">
        <div class="col-md-12">
          <div class ="pull-right">
            <!-- if notice.present? && !current_user.errors.messages.present? -->
            <% if notice.present? %>
            <div class="alert alert-success alert-dismissible fade in" role="alert" id="alert-fade-out">
              <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">Ã—</span>
              </button>
              <strong><%= notice %></strong>
            </div>
            <% end %>
          </div>
        </div>
        <div class="">
          <%= yield %>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
